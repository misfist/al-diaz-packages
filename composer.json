{
    "name": "misfist/al-diaz-packages",
    "description": "WordPress packages for al-diaz.com",
    "type": "project",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Pea",
            "email": "pea@misfist.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
              "name": "misfist/al-diaz",
                "version": "master",
                "type": "wordpress-theme",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/misfist/al-diaz/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "git@github.com:misfist/al-diaz.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "misfist/advanced-custom-fields-pro",
                "version": "v5.5.7",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/misfist/advanced-custom-fields/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/misfist/advanced-custom-fields.git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
      "wpackagist-plugin/codepress-admin-columns": "^2.5.6.4",
      "wpackagist-plugin/admin-management-xtended": "^2.4.1",
      "wpackagist-plugin/admin-post-navigation": "^2.0",
      "misfist/advanced-custom-fields-pro": "v5.5.7",
      "wpackagist-plugin/caldera-forms": "^1.5.0-b-2",
      "wpackagist-plugin/jetpack": "^4.3.2",
      "wpackagist-plugin/multisite-plugin-manager": "^3.1.5",
      "wpackagist-plugin/series": "^1.0.0",
      "wpackagist-plugin/so-widgets-bundle": "^1.8.1",
      "wpackagist-plugin/w3-total-cache": "^0.9.5.2",
      "wpackagist-plugin/wp-postviews": "^1.74",
      "wpackagist-plugin/wp-user-avatar": "^2.0.8",
      "wpackagist-theme/understrap": "^0.5.6",
      "wpackagist-theme/gridbox": "^1.0.9",
      "misfist/al-diaz": "dev-master"
    },
    "require-dev": {
      "wpackagist-plugin/developer": "^1.2.5",
      "wpackagist-plugin/user-switching": "^1.0.9",
      "wpackagist-plugin/which-template": "^4.0",
      "wpackagist-plugin/wordpress-importer": "^0.6.3",
      "wpackagist-plugin/wp-log-viewer": "^1.0.7",
      "squizlabs/php_codesniffer": "^2.5.1"
    },
    "extra": {
      "installer-paths": {
        "../wordpress/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
        "../wordpress/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
        "../wordpress/wp-content/themes/{$name}/": ["type:wordpress-theme"]
      }
    }
}
